data = read.csv(file.choose())
library(DBI)
library(dplyr)
library(dbplyr)
library(odbc)
library(arules)
library(RODBC)
library(RJDBC)

AggPosData = split(Product,Reference)
Txns = as(AggPosData,Reference)
Rules = apriori(AggPosData,parameter =list(supp=0.01,conf=0.01,target='rules',minlen=2))
AprioriFile = DATAFRAME((Rules))
print(AprioriFile)
